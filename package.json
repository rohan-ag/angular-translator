{
  "name": "angular-translator",
  "displayName": "Angular Translator",
  "description": "Angular Translator helps in creating internationalisation file. Currently supported for English, it will save the pain of adding translate pipe for the text and define key pair for that text in en.json file under i18n.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.30.0"
  },
  "categories": [
    "Other",
    "Programming Languages"
  ],
  "keywords": ["Angular", "Translator", "Internationalizer", "i18n", "Angular Translator"],
  "activationEvents": [
    "onCommand:extension.angularTranslate"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.angularTranslate",
        "title": "Angular Translate "
      }
    ],
    "keybindings": [
      {
        "command": "extension.angularTranslate",
        "key": "ctrl+alt+t",
        "mac": "cmd+T",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^3.1.4",
    "vscode": "^1.1.25",
    "@types/node": "^8.10.25",
    "@types/mocha": "^2.2.42"
  }
}